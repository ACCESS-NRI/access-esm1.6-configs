    f_ardg      = 'x', f_vrdg       = 'x',
    f_alvl      = 'x', f_vlvl       = 'x',
    f_dardg1dt  = 'x', f_dardg2dt   = 'x',
    f_dvirdgdt  = 'x', f_opening    = 'x',
